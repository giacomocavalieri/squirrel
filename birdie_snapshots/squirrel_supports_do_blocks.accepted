version: 1.4.0
title: squirrel supports do blocks
file: ./test/squirrel_test.gleam
test_name: squirrel_supports_do_blocks_test
---

import gleam/dynamic/decode
import pog.{type Connection}

pub fn query(db: Connection) {
  let decoder = decode.map(decode.dynamic, fn(_) { Nil })

  "
  do $$ begin
    select 1;
  end $$;
  "
  |> pog.query
  |> pog.returning(decoder)
  |> pog.execute(db)
}

